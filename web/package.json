{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=3.8"
  },
  "scripts": {
    "webpack-ts": "cross-env TS_NODE_PROJECT=\"tools/tsconfig.webpack.json\" webpack --config tools/webpack.config.ts",
    "webpack-dev-server-ts": "cross-env TS_NODE_PROJECT=\"tools/tsconfig.webpack.json\" webpack-dev-server --config tools/webpack.config.ts --progress",
    "start": "yarn run webpack-dev-server-ts --env config_name=integreat-test-cms --env dev_server",
    "start:malte": "yarn run webpack-dev-server-ts --env config_name=malte --env dev_server",
    "start:aschaffenburg": "yarn run webpack-dev-server-ts --env config_name=aschaffenburg --env dev_server",
    "start:integreat-e2e": "yarn run webpack-dev-server-ts --env config_name=integreat-e2e --env dev_server",
    "build": "yarn build:integreat",
    "build:integreat": "yarn run webpack-ts --env config_name=integreat",
    "build:malte": "yarn run webpack-ts --env config_name=malte",
    "build:aschaffenburg": "yarn run webpack-ts --env config_name=aschaffenburg",
    "build:integreat-test-cms": "yarn run webpack-ts --env config_name=integreat-test-cms",
    "check:build": "yarn es-check es2020 ./dist/**/*.js --verbose",
    "test": "jest --config jest.config.ts --maxWorkers=50%",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "lint": " yarn run stylelint && yarn run eslint",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml && yarn run stylelint",
    "eslint:fix": "eslint . --fix",
    "eslint": "eslint .",
    "stylelint": "stylelint --config stylelint.config.js \"src/**/*.{ts,tsx,css}\"",
    "ts:check": "tsc --build",
    "prettier:check": "yarn prettier --ignore-path  ../.prettierignore --check .",
    "prettier:fix": "yarn prettier --ignore-path  ../.prettierignore --write .",
    "circleci:update-config": "yarn --cwd .. circleci:update-config"
  },
