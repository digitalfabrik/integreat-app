  },
  "scripts": {
    "start": "webpack-dev-server --config tools/webpack.config.babel.js --progress --env.config_name=integreat-test-cms --env.dev_server",
    "start:malte": "webpack-dev-server --config tools/webpack.config.babel.js --progress --env.config_name=malte --env.dev_server",
    "start:aschaffenburg": "webpack-dev-server --config tools/webpack.config.babel.js --progress --env.config_name=aschaffenburg --env.dev_server",
    "build": "yarn build:integreat",
    "build:integreat": "webpack --config tools/webpack.config.babel.js --env.config_name=integreat",
    "build:malte": "webpack --config tools/webpack.config.babel.js --env.config_name=malte",
    "build:aschaffenburg": "webpack --config tools/webpack.config.babel.js --env.config_name=aschaffenburg",
    "build:integreat-test-cms": "webpack --config tools/webpack.config.babel.js --env.config_name=integreat-test-cms",
    "check:build": "yarn es-check es5 ./dist/**/*.js --verbose",
    "test": "jest --config jest.config.js",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml && yarn run stylelint",
    "eslint:fix": "eslint . --fix",
    "eslint": "eslint .",
    "stylelint": "stylelint --config stylelint.config.js \"src/**/*.{js,css}\"",
    "flow:check-now": "flow check",
    "flow:install-types": "flow-typed install --skip"
  },
