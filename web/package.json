    "@mui/x-date-pickers": "^8.15.0",
    "@sentry/react": "^10.22.0",
    "@sentry/types": "^10.22.0",
    "build-configs": "0.0.1",
    "core-js": "^3.46.0",
    "dompurify": "^3.3.0",
    "easy-speech": "^2.4.0",
    "i18next": "^25.6.0",
    "luxon": "^3.7.2",
    "maplibre-gl": "^5.10.0",
    "normalize.css": "^8.0.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.21.0",
    "react-i18next": "^16.2.1",
    "react-infinite-scroller": "^1.2.6",
    "react-map-gl": "^8.1.0",
    "react-router": "^7.12.0",
