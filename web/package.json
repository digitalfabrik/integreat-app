    "jest-fetch-mock": "^3.0.3",
    "raf": "^3.4.1",
    "react-refresh": "^0.18.0",
    "react-refresh-typescript": "^2.0.11",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "^9.5.4",
    "tsconfig-paths": "^4.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.2"
  }
}
