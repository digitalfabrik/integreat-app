{
  "name": "native",
  "version": "0.0.1",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "yarn start:integreat-test-cms",
    "start:integreat-test-cms": "cross-env BUILD_CONFIG_NAME='integreat-test-cms' react-native start --reset-cache",
    "start:integreat": "cross-env BUILD_CONFIG_NAME='integreat' react-native start --reset-cache",
    "start:malte": "cross-env BUILD_CONFIG_NAME='malte' react-native start --reset-cache",
    "start:aschaffenburg": "cross-env BUILD_CONFIG_NAME='aschaffenburg' react-native start --reset-cache",
    "start:integreat-e2e": "cross-env BUILD_CONFIG_NAME='integreat-e2e' react-native start --reset-cache",
    "android": "yarn android:integreat-test-cms",
    "android:integreat-test-cms": "cross-env BUILD_CONFIG_NAME='integreat-test-cms' react-native run-android --no-packager --appId app.integreat.test",
    "android:malte": "cross-env BUILD_CONFIG_NAME='malte' react-native run-android --no-packager --appId de.malteapp",
    "android:aschaffenburg": "cross-env BUILD_CONFIG_NAME='aschaffenburg' react-native run-android --no-packager --appId app.aschaffenburg",
    "android:integreat-e2e": "cross-env BUILD_CONFIG_NAME='integreat-e2e' react-native run-android --no-packager",
    "android:integreat:production": "cross-env BUILD_CONFIG_NAME='integreat' react-native run-android --no-packager --variant=release",
    "android:malte:production": "cross-env BUILD_CONFIG_NAME='malte' react-native run-android --no-packager --variant=release --appId de.malteapp",
    "android:aschaffenburg:production": "cross-env BUILD_CONFIG_NAME='aschaffenburg' react-native run-android --no-packager --variant=release --appId app.aschaffenburg",
    "android:reload": "adb shell input text RR",
    "android:menu": "adb shell input keyevent 82",
    "android:reverse": "adb reverse tcp:8081 tcp:8081",
    "android:reinit": "adb shell pm clear com.integreat && adb shell am start com.integreat/com.integreat.MainActivity",
    "android:start-avd": "bash -c '$ANDROID_HOME/emulator/emulator -avd $($ANDROID_HOME/emulator/emulator -list-avds | head -n 1)'",
    "test": "cross-env BUILD_CONFIG_NAME='integreat-test-cms' jest --config ./jest.config.ts --maxWorkers=50%",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "test:e2e": "yarn workspace e2e test:native:browserstack",
    "lint": "yarn run eslint",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml",
    "eslint": "eslint .",
    "postinstall": "jetifier",
    "ts:check": "tsc --build",
    "prettier:check": "yarn prettier --ignore-path  ../.prettierignore --check .",
    "prettier:fix": "yarn prettier --ignore-path  ../.prettierignore --write .",
    "circleci:update-config": "yarn --cwd .. circleci:update-config"
  },
