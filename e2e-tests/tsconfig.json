{
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "sourceMap": false,
    "target": "es6",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/jasmine-framework",
      "@wdio/types",
      "@wdio/selenium-standalone-service",
      "@wdio/browserstack-service",
      "@wdio/junit-reporter"
    ],
    "allowSyntheticDefaultImports": true
  },
}