{
  "version": "0.0.1",
  "name": "e2e",
  "private": true,
  "scripts": {
    "test:web": "yarn wdio run web/wdio.conf.ts",
    "test:web:browserstack": "yarn wdio run ./web/browserstack/wdio-browserstack.conf.ts",
    "test:native:browserstack": "yarn wdio run ./native/wdio-browserstack.conf.ts",
    "test:native": "yarn wdio run native/wdio.conf.ts",
    "prepare:android:native": "yarn workspace native android:integreat-e2e",
    "prepare:start:native": "yarn workspace native start:integreat-e2e",
    "prepare:start:web": "yarn workspace web start:integreat-e2e",
    "lint": "yarn eslint ./",
    "ts:check": "tsc --build"
  },
  "devDependencies": {
    "@wdio/appium-service": "^7.7.3",
    "@wdio/browserstack-service": "^7.7.4",
    "@wdio/cli": "^7.7.4",
    "@wdio/jasmine-framework": "^7.7.3",
    "@wdio/junit-reporter": "^7.7.3",
    "@wdio/local-runner": "^7.7.4",
    "@wdio/selenium-standalone-service": "^7.7.4",
    "@wdio/types": "^7.7.3",
    "appium": "^1.21.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-wdio": "^7.4.2",
    "webdriverio": "^7.7.4"
  },
  "workspaces": {
    "nohoist": [
      "@wdio/**",
      "**/@wdio/**"
    ]
  }
}
