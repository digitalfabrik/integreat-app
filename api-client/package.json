{
  "name": "api-client",
  "version": "0.0.1",
  "description": "Api client for the integreat cms",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "test": "jest --config jest.config.ts --maxWorkers=50%",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "lint": "yarn run eslint .",
    "lint:fix": "yarn run eslint --fix",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml",
    "ts:check": "tsc --build"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "dependencies": {
    "entities": "^3.0.1",
    "htmlparser2": "^7.2.0",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.19",
    "moment": "2.29.1",
    "moment-timezone": "^0.5.34",
    "normalize-path": "^3.0.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.0",
    "@types/js-md5": "^0.4.3",
    "@types/lodash": "^4.14.178",
    "@types/normalize-path": "^3.0.0",
    "@types/react": "^17.0.37",
    "@types/seedrandom": "^3.0.1",
    "jest": "27.4.7",
    "jest-mock": "^27.4.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "seedrandom": "^3.0.3",
    "ts-jest": "^27.1.3"
  }
}
