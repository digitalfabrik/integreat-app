{
  "name": "api-client",
  "version": "0.0.1",
  "description": "Api client for the integreat cms",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint --fix",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml",
    "eslint": "eslint ."
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "js-md5": "^0.7.3",
    "entities": "^2.0.0",
    "moment": "2.25.3",
    "moment-timezone": "^0.5",
    "normalize-path": "^3.0.0",
    "sanitize-html-react": "^1.13",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "seedrandom": "^3.0.3",
    "jest": "^25.5.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0"
  }
}
