{
  "name": "api-client",
  "version": "0.0.1",
  "description": "Api client for the integreat cms",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "test": "jest --config jest.config.ts",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --ci",
    "test:watch": "yarn run test --watchAll",
    "test:update": "yarn run test -u",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint --fix",
    "lint:ci": "yarn run eslint --quiet --format junit -o reports/lint/junit-lint.xml",
    "eslint": "eslint ."
  },
  "dependencies": {
    "js-md5": "^0.7.3",
    "entities": "^2.0.0",
    "moment": "2.29.1",
    "moment-timezone": "^0.5",
    "normalize-path": "^3.0.0",
    "sanitize-html-react": "^1.13",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/js-md5": "^0.4.2",
    "@types/lodash": "^4.14.168",
    "@types/normalize-path": "^3.0.0",
    "@types/seedrandom": "^3.0.0",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "seedrandom": "^3.0.3"
  }
}
