  "scripts": {
    "start": "node tools/run.js",
    "build": "node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "build:lib": "node tools/lib.build.js && flow-copy-source -i '**/@(__mocks__|__tests__)/**' ./src ./dist && mv ./dist/lib.js.flow ./dist/index.js.flow",
    "check:built": "yarn es-check es5 ./www/**/*.js",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --watchAll",
    "test:update": "jest --config jest.config.json -u",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:fix": "yarn run eslint --fix && stylefmt -r \"src/**/*.css\"",
    "eslint": "eslint .",
    "stylelint": "stylelint --config stylelint.config.js \"src/**/*.{js,css}\"",
    "flow:check-now": "flow check",
    "flow:install-types": "flow-typed install --ignoreDeps dev",
    "locales:missing": "node tools/findMissingLocales"
  },
