{
  "name": "integreat-react-native-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env BUILD_CONFIG_NAME='integreat-test-cms' react-native start",
    "start:reset": "env BUILD_CONFIG_NAME='integreat-test-cms' react-native start --reset-cache",
    "start:malte": "env BUILD_CONFIG_NAME='malte' react-native start",
    "android": "react-native run-android --no-packager",
    "android:release": "react-native run-android --no-packager --variant=release",
    "android:reload": "adb shell input text RR",
    "android:menu": "adb shell input keyevent 82",
    "android:reverse": "adb reverse tcp:8081 tcp:8081",
    "android:reinit": "adb shell pm clear com.integreat && adb shell am start com.integreat/com.integreat.MainActivity",
    "android:start-avd": "bash -c '$ANDROID_HOME/emulator/emulator -avd $($ANDROID_HOME/emulator/emulator -list-avds | head -n 1)'",
    "ios": "react-native run-ios --no-packager",
    "test": "env BUILD_CONFIG_NAME='integreat-test-cms' jest --config ./jest.config.js",
    "test:coverage": "env BUILD_CONFIG_NAME='integreat-test-cms' jest --config ./jest.config.js --coverage",
    "test:watch": "env BUILD_CONFIG_NAME='integreat-test-cms' jest --config ./jest.config.js --watchAll",
    "test:update": "env BUILD_CONFIG_NAME='integreat-test-cms' jest --config ./jest.config.js -u",
    "test:e2e": "env BUILD_CONFIG_NAME='integreat-test-cms' jest --config jest.e2e.config.js --maxWorkers=4",
    "lint": "eslint .",
    "flow:check-now": "flow check",
    "flow:install-types": "flow-typed install --ignoreDeps dev",
    "flow:install-types:all": "flow-typed install",
    "postinstall": "jetifier",
    "trigger-pipeline": ".circleci/trigger-pipeline.sh",
    "bump-version": "false",
    "locales:missing": "node locales/tools/findMissingLocales"
  },
  "dependencies": {
    "@integreat-app/integreat-api-client": "^4.3.1",
    "@react-native-community/art": "^1.1.2",
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/netinfo": "^5.5.0",
    "@sentry/react-native": "^1.3.1",
    "@testing-library/react-native": "^5.0.3",
    "hashids": "^2.2.1",
    "htmlparser2": "^4.0.0",
    "i18next": "^19.1.0",
    "is-url": "^1.2.4",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "normalize-path": "^3.0.0",
    "normalize-strings": "^1.1.0",
    "query-string": "^6.11.0",
    "react": "16.9.0",
    "react-i18next": "^11.3.1",
    "react-native": "0.61.5",
    "react-native-elements": "^1.2.7",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "~1.5.6",
    "react-native-highlight-words": "^1.0.1",
    "react-native-image-zoom-viewer": "^2.2.27",
    "react-native-pdf": "^6.0.1",
    "react-native-permissions": "2.0.9",
    "react-native-progress": "^4.0.3",
    "react-native-render-html": "^4.2.0",
    "react-native-share": "^3.0.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^5.3.1",
    "react-navigation": "^3.10.2",
    "react-navigation-header-buttons": "^3.0.5",
    "react-navigation-stack": "^1.4.0",
    "react-redux": "^7.1.3",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "^0.12.0",
    "sanitize-html-react": "^1.13.0",
    "styled-components": "^5.0.1",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@integreat-app/eslint-config-babel": "^0.6.0",
    "@integreat-app/eslint-config-flow": "^0.6.0",
    "@integreat-app/eslint-config-jest": "^0.6.0",
    "@integreat-app/eslint-config-node": "^0.6.0",
    "@integreat-app/eslint-config-react": "^0.6.1",
    "@octokit/auth-app": "^2.4.6",
    "@octokit/rest": "^17.11.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commander": "^5.1.0",
    "eslint": "^6.8.0",
    "flow-bin": "^0.105.2",
    "flow-typed": "^2.6.2",
    "generator-rn-toolbox": "^3.14.0",
    "get-dev-paths": "^0.1.1",
    "http-proxy": "^1.18.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "jest-fetch-mock": "^3.0.1",
    "jest-react-native": "^18.0.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.58.0",
    "node-fetch": "^2.6.0",
    "react-native-git-upgrade": "^0.2.7",
    "react-native-svg-asset-plugin": "^0.3.0",
    "react-test-renderer": "16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.0-rc.1",
    "seedrandom": "^3.0.3",
    "wait-for-expect": "^3.0.2",
    "wd": "^1.12.1",
    "yo": "^3.1.1"
  }
}
