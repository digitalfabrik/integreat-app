{
  "name": "integreat-app",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "native",
      "web",
      "e2e-tests",
      "api-client",
      "tools",
      "translations",
      "build-configs"
    ]
  },
  "scripts": {
    "g:lint": "cd $INIT_CWD && eslint .",
    "g:lint:ci": "yarn g:lint --format junit -o reports/lint/junit-lint.xml",
    "g:lint:changed": "cd $INIT_CWD && eslint . -c ${PROJECT_CWD}/.eslintrc_changed.js",
    "g:eslint": "cd $INIT_CWD && eslint .",
    "g:prettier:check": "cd $INIT_CWD && prettier --check . --ignore-path ${PROJECT_CWD}/.prettierignore",
    "g:prettier:write": "cd $INIT_CWD && prettier --write . --ignore-path ${PROJECT_CWD}/.prettierignore",
    "g:test": "jest",
    "g:test:changed": "yarn g:test -o",
    "g:test:coverage": "yarn g:test --coverage",
    "g:test:ci": "yarn g:test:coverage --ci",
    "g:tsnode": "cd $INIT_CWD && ts-node",
    "g:tsc": "cd $INIT_CWD && tsc --build",
    "circleci:update-config": "{ cat .circleci/autogenerated_header.yml; circleci config pack .circleci/src; } > .circleci/config.yml && circleci config validate"
  },
  "resolutions": {
    "moment": "2.29.4",
    "react": "18.2.0",
    "@types/react": "^18.0.26",
    "@react-native-mapbox-gl/maps@^8.5.0": "patch:@react-native-mapbox-gl/maps@npm%3A8.5.0#./.yarn/patches/@react-native-mapbox-gl-maps-npm-8.5.0-7a0d0c6c9b.patch",
    "react-native-highlight-words@^1.0.1": "patch:react-native-highlight-words@npm%3A1.0.1#./.yarn/patches/react-native-highlight-words-npm-1.0.1-0dfab0c882.patch"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-diff": "^1.0.15",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-jsx-expressions": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^0.0.40",
    "jest": "^29.4.1",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.8.2",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.5.0"
}
