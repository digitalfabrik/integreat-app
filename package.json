{
  "name": "integreat-webapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.8"
  },
  "scripts": {
    "start": "webpack-dev-server --config tools/webpack.config.babel.js --progress --env.config_name=integreat-test-cms --env.debug",
    "start:malte": "webpack-dev-server --config tools/webpack.config.babel.js --progress --env.config_name=malte --env.debug",
    "build": "yarn build:integreat",
    "build:debug": "yarn build:integreat-test-cms",
    "build:integreat": "webpack --config tools/webpack.config.babel.js --env.config_name=integreat --env.production",
    "build:malte": "webpack --config tools/webpack.config.babel.js --env.config_name=malte --env.production",
    "build:integreat-test-cms": "webpack --config tools/webpack.config.babel.js --env.config_name=integreat-test-cms --env.debug",
    "check:build": "yarn es-check es5 ./dist/**/*.js --verbose",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --watchAll",
    "test:update": "jest --config jest.config.json -u",
    "lint": "eslint . && yarn run stylelint",
    "lint:fix": "yarn run eslint --fix && stylefmt -r \"src/**/*.css\"",
    "stylelint": "stylelint --config stylelint.config.js \"src/**/*.{js,css}\"",
    "flow:check-now": "flow check",
    "flow:install-types": "flow-typed install --skip",
    "locales:missing": "node locales/tools/findMissingLocales",
    "circleci:update-config": "cd .circleci && { cat autogenerated_header.yml; circleci config pack src; } > config.yml"
  },
