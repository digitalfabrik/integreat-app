{
  "name": "@integreat-app/integreat-api-client",
  "version": "4.1.2",
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.8"
  },
  "license": "MIT",
  "description": "The client for the integreat api.",
  "author": "Max Ammann <ammann@integreat-app.de>",
  "repository": "https://github.com/integreat/integreat-api-client",
  "files": [
    "*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "integreat",
    "api",
    "client",
    "endpoint"
  ],
  "module": "./index.js",
  "main": "./index-umd.js",
  "sideEffects": true,
  "scripts": {
    "build": "node tools/build.js",
    "npm:publish": "cd dist && npm publish",
    "npm:pack": "cd dist && npm pack && mv *.tgz ../",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:watch": "jest --config jest.config.json --watchAll",
    "test:update": "jest --config jest.config.json -u",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint --fix",
    "eslint": "eslint --ignore-pattern dist/ .",
    "flow:check-now": "flow check",
    "flow:install-types": "flow-typed install"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-do-expressions": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.1.2",
    "@babel/plugin-proposal-json-strings": "^7.1.2",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.1.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.1.2",
    "@babel/plugin-proposal-numeric-separator": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.1.2",
    "@babel/plugin-proposal-pipeline-operator": "^7.1.2",
    "@babel/plugin-proposal-throw-expressions": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.1.2",
    "@babel/plugin-syntax-import-meta": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.2",
    "@babel/preset-env": "^7.1.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@integreat-app/eslint-config-babel": "^0.3.4",
    "@integreat-app/eslint-config-flow": "^0.3.2",
    "@integreat-app/eslint-config-jest": "^0.2.1",
    "@integreat-app/eslint-config-node": "^0.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "es-check": "^5.0.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.91.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "jest": "^24.0.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-junit": "^6.1.0",
    "shelljs": "^0.8.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "entities": "^1.1",
    "is-url": "^1.2",
    "lodash": "^4.17",
    "moment": "^2.24",
    "moment-timezone": "^0.5",
    "sanitize-html-react": "^1.13"
  }
}
