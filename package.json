{
  "name": "integreat-app",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "native",
      "web",
      "e2e-tests",
      "api-client",
      "translations",
      "tools",
      "build-configs"
    ]
  },
  "scripts": {
    ":lint": "cd $INIT_CWD && eslint .",
    ":lint:changed": "eslint . -c $PROJECT_CWD/.eslintrc_changed.js",
    ":lint:ci": "cd $PROJECT_CWD && eslint . --format junit -o $PROJECT_CW/reports/lint/junit-lint.xml",
    ":test": "cd $INIT_CWD && jest",
    ":test:changed": "cd $INIT_CWD && jest",
    ":test:ci": "cd $PROJECT_CWD && jest --coverage --ci",
    "circleci:update-config": "{ cat .circleci/autogenerated_header.yml; circleci config pack .circleci/src; } > .circleci/config.yml && circleci config validate",
    "prettier:check": "prettier --ignore-path .prettierignore --check $PWD",
    "prettier:write": "prettier --ignore-path .prettierignore --write $PWD",
    "ts:check": "cd $INIT_CWD && tsc"
  },
  "resolutions": {
    "luxon": "^3.4.2",
    "react": "18.2.0",
    "react-native-highlight-words@^1.0.1": "patch:react-native-highlight-words@npm%3A1.0.1#./.yarn/patches/react-native-highlight-words-npm-1.0.1-0dfab0c882.patch"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-diff": "^2.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-jsx-expressions": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^2.1.31",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.6.3"
}
